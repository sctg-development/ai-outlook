{
  "name": "sctg-ai-pane",
  "version": "0.0.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React.git"
  },
  "license": "AGPL-3.0-only",
  "config": {
    "app_to_debug": "outlook",
    "app_type_to_debug": "webpack",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "npm run getPrompts && npm run getGroqModels && npx webpack --mode production",
    "build:github-pages": "npm run getGroqModels && npx webpack --env website=GITHUB_PAGES --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "getGroqModels": "tsx utils/initModels.ts",
    "getPrompts": "tsx utils/getPrompts.ts",
    "_getGroqModels": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./utils/initModels.ts",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "preview": "http-server ./dist -p 3000",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "office-addin-debugging start manifest.json --app outlook",
    "stop": "office-addin-debugging stop manifest.json",
    "validate": "office-addin-manifest validate manifest.json",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.58.3",
    "@fluentui/react-icons": "^2.0.274",
    "@sctg/ai-sdk": "0.0.4",
    "@sctg/sentencepiece-js": "^1.3.3",
    "core-js": "^3.40.0",
    "dompurify": "^3.2.4",
    "es6-promise": "^4.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^7.1.5",
    "regenerator-runtime": "^0.14.1",
    "rehype-highlight": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/office-js": "^1.0.465",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "acorn": "^8.14.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint-plugin-office-addins": "^3.0.3",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "http": "^0.0.1-security",
    "http-server": "^14.1.1",
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "simple-git": "^3.27.0",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.0",
    "webpack-shell-plugin-next": "^2.3.2"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "last 2 versions",
    "not dead"
  ]
}
